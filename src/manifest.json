{
  "manifest_version": 3,
  "name": "Streamer Pronouns",
  "description": "Display Pronouns on Twitch Channels",
  "author": "Y.Heckmann",
  "version": "1.0",
  "icons": {
    "128": "icons/icon128.png",
    "48": "icons/icon48.png",
    "16": "icons/icon16.png"
  },
  "content_scripts": [
    {
      "matches": ["*://www.twitch.tv/*", "*://twitch.tv/*"],
      "js": ["bundle.js"],
      "css": ["style.css"]
    }
  ]
}
